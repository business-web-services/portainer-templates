{
  "version": "3",
  "templates": [
    {
      "id": 1,
      "type": 1,
      "title": "FastAPI",
      "description": "Docker image for FastAPI app.",
      "categories": ["health"],
      "platform": "linux",
      "logo": "https://raw.githubusercontent.com/rserverd/templates/main/stacks/fastapi/logo/logo.png",
      "repository": {
        "url": "https://github.com/rserverd/templates",
        "stackfile": "stacks/fastapi/docker-stack.yml"
      },
      "env": [
        {
          "name": "TIMEZONE",
          "label": "Timezone",
          "default": "America/Santo_Domingo"
        },
        {
          "name": "FASTAPI_IMAGE",
          "label": "FastAPI Image",
          "default": "code.rserverd.com/gruposolutema/fastapi"
        },
        {
          "name": "FASTAPI_TAG",
          "label": "FastAPI Tag",
          "default": "latest"
        },
        {
          "name": "URL",
          "label": "URL",
          "default": "demo.example.com"
        },
        {
          "name": "NETWORK",
          "label": "Network",
          "default": "proxy"
        }
      ],
      "ports": ["8000/tcp"],
      "volumes": [
        {
          "container": "/code"
        }
      ]
    },
    {
      "id": 2,
      "type": 1,
      "title": "GNU Health",
      "description": "Docker image for GNU Health a free/libre health and hospital information system.",
      "categories": ["health"],
      "platform": "linux",
      "logo": "https://raw.githubusercontent.com/rserverd/templates/main/stacks/gnuhealth/logo/logo.png",
      "repository": {
        "url": "https://github.com/rserverd/templates",
        "stackfile": "stacks/gnuhealth/docker-stack.yml"
      },
      "env": [
        {
          "name": "TIMEZONE",
          "label": "Timezone",
          "default": "America/Santo_Domingo"
        },
        {
          "name": "POSTGRES_IMAGE",
          "label": "Postgres Image",
          "default": "code.rserverd.com/gruposolutema/postgres"
        },
        {
          "name": "POSTGRES_TAG",
          "label": "Postgres Tag",
          "default": "16.6"
        },
        {
          "name": "DB_NAME",
          "label": "Database Name"
        },
        {
          "name": "DB_USER",
          "label": "Database User"
        },
        {
          "name": "DB_PASS",
          "label": "Database Password for User"
        },
        {
          "name": "SUPERUSER_PASS",
          "label": "Database Password for Root"
        },

        {
          "name": "DB_EXTENSION",
          "label": "Database extensions",
          "default": "unaccent"
        },
        {
          "name": "GNUHEALTH_IMAGE",
          "label": "GNU Health Image",
          "default": "code.rserverd.com/gruposolutema/gnuhealth"
        },
        {
          "name": "GNUHEALTH_TAG",
          "label": "GNU Health Tag",
          "default": "latest"
        },
        {
          "name": "GNUHEALTH_LANGUAGE",
          "label": "GNU Health Language",
          "default": "es_419"
        },
        {
          "name": "EMAIL",
          "label": "E-Mail",
          "default": "admin@example.com"
        },
        {
          "name": "PASSWORD",
          "label": "Password",
          "default": "admin"
        },
        {
          "name": "URL",
          "label": "URL",
          "default": "demo.example.com"
        },
        {
          "name": "NETWORK",
          "label": "Network",
          "default": "proxy"
        }
      ],
      "ports": ["8000/tcp"],
      "volumes": [
        {
          "container": "/var/lib/postgresql/data"
        },
        {
          "container": "/var/lib/trytond/db"
        }
      ]
    }
  ]
}
