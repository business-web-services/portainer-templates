          "name": "GITEA_USER"
        },
        {
          "default": "Password",
          "label": "Gitea SMTP Password",
          "name": "GITEA_PASSWD"
        },
        {
          "default": "code.example.com",
          "label": "Domain",
          "name": "DOMAIN"
        },
        {
          "default": "proxy",
