{
  "version": "3",
  "templates": [
    {
      "id": 1,
      "type": 3,
      "title": "FastAPI",
      "description": "Docker image for FastAPI app.",
      "categories": ["health"],
      "platform": "linux",
      "logo": "https://raw.githubusercontent.com/rserverd/templates/main/stacks/fastapi/logo/logo.png",
      "repository": {
        "url": "https://github.com/rserverd/templates",
        "stackfile": "stacks/fastapi/docker-compose.yml"
      },
      "env": [
        {
          "name": "TIMEZONE",
          "label": "Timezone",
          "default": "America/Santo_Domingo"
        },
        {
          "name": "FASTAPI_IMAGE",
          "label": "FastAPI Image",
          "default": "code.rserverd.com/gruposolutema/fastapi"
        },
        {
          "name": "FASTAPI_TAG",
          "label": "FastAPI Tag",
          "default": "latest"
        },
        {
          "name": "URL",
          "label": "URL",
          "default": "demo.example.com"
        },
        {
          "name": "NETWORK",
          "label": "Network",
          "default": "proxy"
        }
      ],
      "ports": ["8000/tcp"],
      "volumes": [
        {
          "container": "/code"
        }
      ]
    },
    {
      "id": 2,
      "type": 3,
      "title": "GNU Health",
      "description": "Docker image for GNU Health a free/libre health and hospital information system.",
      "categories": ["health"],
      "platform": "linux",
      "logo": "https://raw.githubusercontent.com/rserverd/templates/main/stacks/gnuhealth/logo/logo.png",
      "repository": {
        "url": "https://github.com/rserverd/templates",
        "stackfile": "stacks/gnuhealth/docker-compose.yml"
      },
      "env": [
        {
          "name": "TIMEZONE",
          "label": "Timezone",
          "default": "America/Santo_Domingo"
        },
        {
          "name": "POSTGRES_IMAGE",
          "label": "Postgres Image",
          "default": "code.rserverd.com/gruposolutema/postgres"
        },
        {
          "name": "POSTGRES_TAG",
          "label": "Postgres Tag",
          "default": "16.6"
        },
        {
          "name": "DB_NAME",
          "label": "Database Name"
        },
        {
          "name": "DB_USER",
          "label": "Database User"
        },
        {
          "name": "DB_PASS",
          "label": "Database Password for User"
        },
        {
          "name": "SUPERUSER_PASS",
          "label": "Database Password for Root"
        },

        {
          "name": "DB_EXTENSION",
          "label": "Database extensions",
          "default": "unaccent"
        },
        {
          "name": "GNUHEALTH_IMAGE",
          "label": "GNU Health Image",
          "default": "code.rserverd.com/gruposolutema/gnuhealth"
        },
        {
          "name": "GNUHEALTH_TAG",
          "label": "GNU Health Tag",
          "default": "latest"
        },
        {
          "name": "GNUHEALTH_LANGUAGE",
          "label": "GNU Health Language",
          "default": "es_419"
        },
        {
          "name": "EMAIL",
          "label": "E-Mail",
          "default": "admin@example.com"
        },
        {
          "name": "PASSWORD",
          "label": "Password",
          "default": "admin"
        },
        {
          "name": "URL",
          "label": "URL",
          "default": "demo.example.com"
        },
        {
          "name": "NETWORK",
          "label": "Network",
          "default": "proxy"
        }
      ],
      "ports": ["8000/tcp"],
      "volumes": [
        {
          "container": "/var/lib/postgresql/data"
        },
        {
          "container": "/var/lib/trytond/db"
        }
      ]
    },
    {
      "id": 3,
      "type": 3,
      "title": "PostgreSQL",
      "description": "Docker image for PostgreSQL.",
      "categories": ["health"],
      "platform": "linux",
      "logo": "https://raw.githubusercontent.com/rserverd/templates/main/stacks/postgres/logo/logo.png",
      "repository": {
        "url": "https://github.com/rserverd/templates",
        "stackfile": "stacks/postgres/docker-compose.yml"
      },
      "env": [
        {
          "name": "TIMEZONE",
          "label": "Timezone",
          "default": "America/Santo_Domingo"
        },
        {
          "name": "POSTGRES_IMAGE",
          "label": "Postgres Image",
          "default": "code.rserverd.com/gruposolutema/postgres"
        },
        {
          "name": "POSTGRES_TAG",
          "label": "Postgres Tag",
          "default": "16.6"
        },
        {
          "name": "INITDB_LOCALE",
          "label": "Init Locale",
          "default": "en"
        },
        {
          "name": "INITDB_ENCODING",
          "label": "Init Encoding",
          "default": "UTF-8"
        },
        {
          "name": "INITDB_LC_COLLATE",
          "label": "Init LC Collate",
          "default": "C"
        },
        {
          "name": "INITDB_LC_CTYPE",
          "label": "Init LC CType",
          "default": "C"
        },
        {
          "name": "DB_NAME",
          "label": "Database Name",
          "default": "postgres_db"
        },
        {
          "name": "DB_USER",
          "label": "Database Username",
          "default": "postgres_user"
        },
        {
          "name": "DB_PASS",
          "label": "Database Password",
          "default": "postgres_password"
        },
        {
          "name": "SUPERUSER_USER",
          "label": "Database Root Username",
          "default": "postgres_root_user"
        },
        {
          "name": "SUPERUSER_PASS",
          "label": "Database Root Password",
          "default": "postgres_root_password"
        },
        {
          "name": "DB_EXTENSION",
          "label": "Database Extensions",
          "default": "unaccent"
        },
        {
          "name": "LISTEN_IP",
          "label": "Listen IP",
          "default": "*"
        },
        {
          "name": "LISTEN_PORT",
          "label": "Listen Port",
          "default": "5432"
        },
        {
          "name": "ENABLE_REPLICATION",
          "label": "Enable Replication",
          "default": "FALSE"
        },
        {
          "name": "REPLICATION_IP_ALLOW",
          "label": "Replication IP Allow",
          "default": "0.0.0.0/0"
        },
        {
          "name": "REPLICATION_MODE",
          "label": "Replication Mode",
          "default": "main"
        },
        {
          "name": "REPLICATION_HOST",
          "label": "Replication Host",
          "default": "localhost"
        },
        {
          "name": "REPLICATION_PORT",
          "label": "Replication Port",
          "default": "5432"
        },
        {
          "name": "REPLICATION_TLS_MODE",
          "label": "Replication TLS Mode",
          "default": "prefer"
        },
        {
          "name": "REPLICATION_USER",
          "label": "Replication Username",
          "default": "replicate"
        },
        {
          "name": "REPLICATION_PASS",
          "label": "Replication Password",
          "default": "replicate"
        },
        {
          "name": "CONTAINER_ENABLE_MONITORING",
          "label": "Enable Container Monitoring",
          "default": "TRUE"
        },
        {
          "name": "MONITOR_USER",
          "label": "Monitor Username",
          "default": "zbx_monitor"
        },
        {
          "name": "MONITOR_PASS",
          "label": "Monitor Password",
          "default": "zabbix"
        },
        {
          "name": "URL",
          "label": "URL",
          "default": "postgres.example.com"
        },
        {
          "name": "POSTGRES_PORT",
          "label": "Postgres Port",
          "default": "5432"
        },
        {
          "name": "NETWORK",
          "label": "Network",
          "default": "proxy"
        }
      ],
      "ports": ["5432/tcp"],
      "volumes": [
        {
          "container": "/var/lib/postgresql/data"
        },
        {
          "container": "/logs"
        }
      ]
    }
  ]
}
