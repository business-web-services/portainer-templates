{
  "version": "3",
  "templates": [
    {
      "id": 1,
      "type": 3,
      "title": "FastAPI",
      "description": "Docker image for FastAPI app.",
      "categories": ["Web Framework"],
      "platform": "linux",
      "logo": "https://raw.githubusercontent.com/rserverd/templates/main/stacks/fastapi/logo/logo.png",
      "repository": {
        "url": "https://github.com/rserverd/templates",
        "stackfile": "stacks/fastapi/docker-compose.yml"
      },
      "env": [
        {
          "name": "TIMEZONE",
          "label": "Timezone",
          "default": "America/Santo_Domingo"
        },
        {
          "name": "FASTAPI_IMAGE",
          "label": "FastAPI Image",
          "default": "code.rserverd.com/gruposolutema/fastapi"
        },
        {
          "name": "FASTAPI_TAG",
          "label": "FastAPI Tag",
          "default": "1.0.0"
        },
        {
          "name": "OVPN_AUTH",
          "label": "OpenVPN Auth"
        },
        {
          "name": "OVPN_KEY_PASS",
          "label": "OpenVPN Key Pass"
        },
        {
          "name": "DB_USER",
          "label": "Database Username"
        },
        {
          "name": "DB_PASSWORD",
          "label": "Database Password"
        },
        {
          "name": "DB_HOST",
          "label": "Database Host"
        },
        {
          "name": "DB_PORT",
          "label": "Database Host Port"
        },
        {
          "name": "DB_NAME",
          "label": "Database Name"
        },
        {
          "name": "PRODUCT_PRICE_DECIMAL",
          "label": "Product Price Decimal"
        },
        {
          "name": "URL",
          "label": "URL",
          "default": "demo.example.com"
        },
        {
          "name": "NETWORK",
          "label": "Network",
          "default": "proxy"
        }
      ],
      "ports": ["80/tcp"],
      "volumes": [
        {
          "container": "/code"
        }
      ]
    },
      {
      "id": 2,
